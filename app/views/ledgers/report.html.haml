%table{:class => "table table-striped table-hover "}
  %tr
    %th #
    -@column_count.times do |i|
      %th=@column_headers[i]
    %th Total
        
  -@row_count.times do |j|
    %tr
      %td=@row_headers[j]
      -@column_count.times do |i|
        %td= @row1a[0].ammount                         
      %td= @row1.total
           
  %tr{:class => "info"}
    %td Total Wealth
    -@column_count.times do |i|
      %td=@column_total[i]
    %td= @g_total

%p
  =@row1a[0].ammount
  =@row1_table 